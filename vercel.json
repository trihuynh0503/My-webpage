{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@now/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
    }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        },
      {
        "src": "/(.*)",
        "dest": "server.js"
      },

    ]
  }